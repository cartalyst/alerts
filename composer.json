{
    "name": "cartalyst/alerts",
    "description": "Alerts allows you to easily pass alert messages to your views.",
    "keywords": [
        "php",
        "cartalyst",
        "laravel",
        "messages",
        "flash",
        "alerts"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "cartalyst/php-cs-fixer-config": "^1.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "suggest": {
        "illuminate/session": "Required to send flash messages across requests. (~4.1)",
        "illuminate/filesystem": "Required to send flash messages across requests. (~4.1)"
    },
    "autoload": {
        "psr-4": {
            "Cartalyst\\Alerts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cartalyst\\Alerts\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
